# Semaphore UI Environment Configuration
# Copy this file to .env and update with your values

# Database Configuration
MYSQL_PASSWORD=your_secure_mysql_password

# Semaphore Admin Configuration
SEMAPHORE_ADMIN_PASSWORD=your_secure_admin_password
SEMAPHORE_ADMIN_NAME=admin
SEMAPHORE_ADMIN_EMAIL=admin@yourcompany.com
SEMAPHORE_ADMIN=admin

# Network Configuration
SEMAPHORE_PORT=3000
SEMAPHORE_WEB_HOST=http://localhost:3000

# Security (Generate new key with: head -c32 /dev/urandom | base64)
SEMAPHORE_ACCESS_KEY_ENCRYPTION=your_generated_encryption_key_here